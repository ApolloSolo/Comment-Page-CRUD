    <body>
        <h1>All Comments</h1>

        <ul>
            <% for(let c of comments){ %>
                    <li><b><%= c.username %> -</b> <%= c.comment %> <a href="/comments/<%= c.id %>">details</a> </li>
            <% } %>
        </ul>

        <a href="/comments/new">New Comment</a>
        
        <script src="" async defer></script>
    </body>
